{
  "categories": [
    {
      "title": "## 🚀 Features",
      "labels": ["feature"]
    },
    {
      "title": "## 🐛 Fixes",
      "labels": ["fix"]
    },
    {
      "title": "## 🧪 Tests",
      "labels": ["test"]
    },
    {
      "title": "## \uD83E\uDDF0 Dependencies",
      "labels": ["dependencies"]
    },
    {
      "title": "## 🧪 Tests and some 🪄 Magic",
      "labels": ["test", "magic"],
      "exclude_labels": ["no-magic"],
      "exhaustive": true
    }
  ],
  "sort": {
    "order": "ASC",
    "on_property": "mergedAt"
  },
  "template": "\n\nDiff: ${{RELEASE_DIFF}}\n\n${{CHANGELOG}}",
  "pr_template": "- (${{MERGE_SHA}}) ${{TITLE}}",
  "empty_template": "- no changes"
}